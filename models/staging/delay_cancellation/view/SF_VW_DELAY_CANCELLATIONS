create or replace view PC_DBT_DB.DBT_JWALC.VW_VW_DELAY_CANCELATIONS(
	FL_DATE,
	AIRLINE_CODE,
	DOT_CODE,
	FL_NUMBER,
	ORIGIN,
	ORIGIN_CITY,
	DEST,
	DEST_CITY,
	CRS_DEP_TIME,
	DEP_TIME,
	DEP_DELAY,
	TAXI_OUT,
	WHEELS_OFF,
	WHEELS_ON,
	TAXI_IN,
	CRS_ARR_TIME,
	ARR_TIME,
	ARR_DELAY,
	CANCELLED,
	CANCELLATION_CODE,
	DIVERTED,
	CRS_ELAPSED_TIME,
	ELAPSED_TIME,
	AIR_TIME,
	DISTANCE,
	DELAY_DUE_CARRIER,
	DELAY_DUE_WEATHER,
	DELAY_DUE_NAS,
	DELAY_DUE_SECURITY,
	DELAY_DUE_LATE_AIRCRAFT,
	FL_YEAR,
	FL_MONTH,
	FL_DAY
) as (
    


with YEAR2021 as (

    select * from PC_DBT_DB.dbt_JWalc.VW_YEAR2021

),

YEAR2022 as (

    select * from PC_DBT_DB.dbt_JWalc.VW_YEAR2022

),

YEAR2023 as (

    select * from PC_DBT_DB.dbt_JWalc.VW_YEAR2023

),

DELAY_CANCELLATIONS as (
        select * from YEAR2021
            union all
        select * from VW_YEAR2022
            union all
        select * from YEAR2023

)

select * from DELAY_CANCELLATIONS
  );
